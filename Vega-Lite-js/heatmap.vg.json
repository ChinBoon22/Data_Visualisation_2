{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Heatmap",
  "data": {
    "url": "../Dataset/6_top_20_ozone_country.csv"
  },
  "width": 600,
  "height": 300,
  "title": "Air Quality Ozone for Top 20 Highest ",
  "mark": "rect",
  "encoding": {
    "x": {
      "field": "last_updated_day",
      "type": "ordinal",
      "title": "Date",
      "timeUnit": "binnedutcyearmonthdate"
    },
    "y": {
      "field": "country",
      "type": "ordinal",
      "title": "Country"
    },
    "color": {
      "field": "air_quality_Ozone",
      "type": "quantitative",
      "legend": {
        "title": "Air Quality Ozone"
      }
    }
  }
}
