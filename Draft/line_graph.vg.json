{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400,
  "height": 400,
  "description": "Stock prices of 5 Tech Companies over Time.",
  "data": { "url": "../Dataset/aggregated_data_for_stream_graph.csv" },
  "mark": "line",
  "encoding": {
    "y": { "field": "gust_mph", "type": "quantitative" },
    "x": { "field": "last_updated_day", "type": "ordinal" },
    "color": { "field": "continent", "type": "nominal" }
  }
}
